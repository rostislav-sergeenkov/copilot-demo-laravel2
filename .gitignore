# Laravel .gitignore

# Dependencies
/laravel-app/vendor/
/laravel-app/node_modules/

# Environment files
/laravel-app/.env
/laravel-app/.env.backup
/laravel-app/.env.production

# IDE & Editor files
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store
Thumbs.db

# Laravel specific
/laravel-app/storage/*.key
/laravel-app/storage/framework/cache/data/*
!/laravel-app/storage/framework/cache/data/.gitignore
/laravel-app/storage/framework/sessions/*
!/laravel-app/storage/framework/sessions/.gitignore
/laravel-app/storage/framework/testing/*
!/laravel-app/storage/framework/testing/.gitignore
/laravel-app/storage/framework/views/*
!/laravel-app/storage/framework/views/.gitignore
/laravel-app/storage/logs/*
!/laravel-app/storage/logs/.gitignore

# Database
/laravel-app/database/*.sqlite
/laravel-app/database/*.sqlite-journal

# Compiled assets
/laravel-app/public/hot
/laravel-app/public/storage
/laravel-app/public/build

# NPM & build files
npm-debug.log
yarn-error.log
/laravel-app/mix-manifest.json

# Composer
composer.phar

# Testing
/laravel-app/.phpunit.result.cache
/laravel-app/.phpunit.cache

# Auth
/laravel-app/auth.json
